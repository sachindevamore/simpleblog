{
  "compilerOptions": {
  "target": "es6",
  "module": "commonjs",
  "outDir": "dist",
  "sourceMap": true
  },
  "moduleResolution": "node" /* Specify module resolution strategy. */,
  //"baseUrl": "./" /* Base directory to resolve non-absolute module names. */,
  "paths": {
    "*": ["node_modules/", "src/types/*"]
  } ,
  "esModuleInterop": true ,
  "include": [
  "src/**/*.ts"
  ],
  "exclude": [
  "node_modules",
  ".vscode"
  ]
  }